w={1<->2,2<->3,3<->1,  1<->4};
et={1,2,3,4};
e=Graph[w, EdgeWeight -> et, EdgeLabels->Table[w[[i]]->et[[i]],{i,Length[w]}],VertexLabels->"Name"]


